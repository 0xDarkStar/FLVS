#My first time messing with the turtle module

import turtle

def main():

    tom = turtle.Turtle()
    tom.left(45)
	tom.forward(50)
	a = 0
	while a <= 6:
		tom.left(25.7)
		tom.forward(10)
		a += 1
	tom.right(115)
	a = 0
	while a <= 6:
		tom.left(25.7)
		tom.forward(10)
		a += 1
	tom.left(29)
	tom.forward(54)
	tom.left(87)
	tom.forward(30)
	tom.right(90)
	tom.forward(15)
	tom.left(25)
	tom.forward(10)
	tom.right(25)
	tom.forward(20)
	tom.right(155)
	tom.forward(10)
	tom.right(25)
	tom.forward(20)
	tom.left(180)
	tom.right(25)
	tom.forward(10)
	tom.left(25)
	tom.forward(20)
	tom.left(155)
	tom.forward(10)
	tom.left(25)
	tom.forward(60)
main()
